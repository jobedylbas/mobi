      <!-- Area Chart Example-->
      <div class="card mb-3">
        <div class="card-header">
          <div class="align-middle float-left">
            <i class="fa fa-area-chart"></i> Area Chart Example
          </div>
          <div id="controllers" class="row col-lg-5 float-right">
            <div class="col-sm-2 btn-group btn-group-sm mr-3">
              <button class="btn btn-secondary">
                <i class="fa fa-cog"></i>
              </button>
              <button class="btn btn-secondary" id="download">
                <i class="fa fa-download"></i>
              </button>
            </div>
            <div class="col-sm-5 btn-group btn-group-sm mr-5" id="granularity">
              <button class="btn btn-secondary" id="10">
                10
              </button>
              <button class="btn btn-secondary" id="30">
                30
              </button>
              <button class="btn btn-secondary" id="60">
                60
              </button>
              <button class="btn btn-secondary" id="120">
                120
              </button>
              <button class="btn btn-secondary" id="0">
                All time
              </button>
            </div>
            <div class="col-sm-2 btn-group btn-group-sm float-right">
              <button class="btn btn-secondary" id="area">
                <i class="fa fa-area-chart" aria-hidden="true"></i>
              </button>
              <button class="btn btn-secondary" id="bar">
                <i class="fa fa-bar-chart-o" aria-hidden="true"></i>
              </button>
              <button class="btn btn-secondary" id="line">
                <i class="fa fa-line-chart" aria-hidden="true"></i>
              </button>
            </div>
          </div>
        </div>
        <div class="card-body">
          <canvas id="myAreaChart" width="100%" height="30"></canvas>
        </div>
        <div class="card-footer small text-muted">
          <div class="row">
            <div class="col-sm-4">
              <i class="fa fa-arrow-down" aria-hidden="true"></i>
              <span>Min: </span>
            </div>
            <div class="col-sm-4">
              <i class="fa fa-minus" aria-hidden="true"></i>
              <span>Av: </span>
            </div>
            <div class="col-sm-4 ">
              <i class="fa fa-arrow-up" aria-hidden="true"></i>
              <span>Max: </span>
            </div>
          </div>
        </div>
      </div>